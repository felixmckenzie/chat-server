    "graphql-shield": "^7.6.5",
    "graphql-subscriptions": "^2.0.0",
    "graphql-ws": "^5.12.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.1",
    "supertest": "^6.3.3",
    "svix": "^0.84.1",
    "ws": "^8.12.1"
  }
