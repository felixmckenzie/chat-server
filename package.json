{
  "name": "server",
  "version": "1.0.0",
  "description": "Dev Chat API",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "concurrently -n graphql-codegen,ts-node 'graphql-code-generator -w' 'ts-node-dev --no-notify --respawn --transpile-only src/index.ts'",
    "build": "rm -rf dist/* && tsc",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts",
    "test": "dotenv -e .env.test -- jest -i",
    "generate": "graphql-codegen --config codegen.yml",
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "Felix McKenzie",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/typescript": "3.0.0",
    "@graphql-codegen/typescript-resolvers": "3.0.0",
    "@types/cors": "^2.8.13",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.13.0",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "babel-jest": "^29.5.0",
    "bcrypt": "^5.1.0",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.1.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "graphql-tag": "^2.12.6",
    "jest": "^29.5.0",
    "jest-environment-node": "^29.5.0",
    "nanoid": "^4.0.2",
    "prettier": "^2.8.4",
    "prisma": "^4.13.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@apollo/server": "^4.4.0",
    "@clerk/clerk-sdk-node": "^4.8.2",
    "@graphql-tools/schema": "^9.0.16",
    "@prisma/client": "^4.13.0",
    "auth0": "^3.3.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "graphql": "^16.6.0",
    "graphql-middleware": "^6.1.33",
